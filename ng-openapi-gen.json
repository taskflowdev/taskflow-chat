{
  "input": "http://localhost:32650/swagger/v1/swagger.json",
  "output": "src/app/api",
  "ignoreUnusedModels": true,
  "generateServices": true,
  "serviceSuffix": "Service",
  "modelSuffix": "",
  "useUnionTypes": true,
  "clearOutputFolder": true,
  "httpClientType": "HttpClient",
  "useClass": false,
  "noImplicitAdditionalProperties": false,
  "generateApiModule": true,
  "moduleName": "ApiModule",
  "exportModels": true,
  "exportServices": true
}
