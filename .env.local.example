# Example environment variables for local development
# Copy this file to .env.local and update with your values

# API Base URL - where your backend API is hosted
API_URL=https://localhost:44347

# Encryption Key - used to encrypt/decrypt data in localStorage
# IMPORTANT: Change this to a secure random key in production!
ENCRYPTION_KEY=taskflow-chat-secure-key-2024

# Production mode flag
PRODUCTION=false
